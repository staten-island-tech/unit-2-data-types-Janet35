bank_balance = 0

while True:
    deposit = input("Enter amount of money: ")

    if deposit == "u got any??":
        print("bru get yo bum ahh out of here scram skedaddle")
        break

    elif int(deposit) <= 1000:
        bank_balance = bank_balance + int(deposit)
        print(bank_balance)
    
    elif int(deposit) > 1000:
        print("ayo g where u got all this money from?! split it w a homie")
